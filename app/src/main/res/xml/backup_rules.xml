<?xml version="1.0" encoding="utf-8"?>
<full-backup-content>
    <!-- Exclude sensitive user data from backups -->
    <exclude domain="datastore" path="." />
    <exclude domain="sharedpref" path="skyzone_preferences.xml" />
    <exclude domain="sharedpref" path="cart_preferences.xml" />
    <exclude domain="database" path="skyzone_database.db" />
    <!-- Include cache for better user experience -->
    <include domain="file" path="." />
</full-backup-content>
