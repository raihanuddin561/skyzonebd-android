<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude DataStore files from cloud backups -->
        <exclude domain="file" path="datastore" />
        <!-- Include other app data -->
        <include domain="file" path="." />
    </cloud-backup>
    
    <device-transfer>
        <!-- Exclude DataStore files from device transfer -->
        <exclude domain="file" path="datastore" />
    </device-transfer>
</data-extraction-rules>
